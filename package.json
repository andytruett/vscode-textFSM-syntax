{
	"name": "TextFSM-Syntax",
	"displayName": "TextFSM Template Syntax",
  "description": "Syntax highlighting for TextFSM templates",
  "icon": "icon.png",
  "version": "1.0.2",
  "publisher": "AndyTruett",
  "repository": {
    "type": "git",
    "url": "https://github.com/andytruett/vscode-textFSM-syntax.git"
  },
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Programming Languages"
	],
	"contributes": {
    "languages": [{
      "id": "textFSM",
      "extensions": [".template"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "textFSM",
      "scopeName": "text.textFSM",
      "path": "./syntaxes/textFSM.tmlanguage.json"
    }]
  }
}
